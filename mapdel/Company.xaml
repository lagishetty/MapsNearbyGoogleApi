        <Button Content="Response" HorizontalAlignment="Left" Margin="235,139,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <ListView HorizontalAlignment="Left" Name="lv1" ItemsSource="{Binding value}"  Height="203" Margin="52,299,0,0" VerticalAlignment="Top" Width="116"/>
        <TextBlock x:Name="tbname" HorizontalAlignment="Left" Margin="52,39,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="48" Width="292" FontSize="20"/>
        <TextBlock HorizontalAlignment="Left" Margin="52,92,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="31" Width="292" FontSize="16"/>
        <AppBarButton HorizontalAlignment="Left" Icon="Accept" Label="Logout" Margin="315,511,0,0" VerticalAlignment="Top" Click="AppBarButton_Click"/>
        <ListView x:Name="lv2" HorizontalAlignment="Left" Height="203" Margin="206,299,0,0" VerticalAlignment="Top" Width="138"/>

    </Grid>
</Page>
